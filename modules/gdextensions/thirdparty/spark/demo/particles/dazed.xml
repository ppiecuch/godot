<?xml version="1.0" encoding="iso-8859-1"?>
<ParticleTrigger xmlns="http://intranet.twotribes.com/schemas/common/particletrigger.xsd" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://intranet.twotribes.com/schemas/common/particletrigger.xsd http://intranet.twotribes.com/schemas/common/particletrigger.xsd">
	<GameEvent name="dazed" max_particles="200"/>
	<ParticleCulling enabled="true"/>
	<ParticleEmitter max_particles="4" x="0" y="0" delay="0" space="local" ignore_fog="false">
		<RenderGroup value="350"/>
		<Texture filename="particles/dazed_pop.png"/>
		<FrameAnimations framesize="64">
			<Animation start="0" end="9" fps="30" type="loop"/>
		</FrameAnimations>
		<EmissionCircle radius="1.2" innerradius="0.5"/>
		<ExternalForce min_x="0" max_x="0" min_y="0" max_y="0"/>
		<BlendMode type="add"/>
		<BlendModeAlpha type="lightmask"/>
		<PreGeneration time="0" step="0"/>
		<Emission particles="10" type="timed" lifetime="5"/>
		<ParticleOrientation type="rotation"/>
		<Velocity min_x="0" max_x="0" min_y="0" max_y="0"/>
		<VelocityFriction min="0" max="0"/>
		<ParticleLife min="0.3" max="0.3"/>
		<Size min_start="0.575" max_start="0.925"/>
		<Weight min_start="1" max_start="1" min_end="0" max_end="0"/>
		<Rotation min_start="-180" max_start="180" min_speed="0" max_speed="0"/>
		<RotationForce min="0" max="0"/>
		<RotationFriction min="0" max="0"/>
		<Frame min_start="0" max_start="10"/>
		<MinStartColor r="255" g="255" b="255" a="143"/>
		<MaxStartColor r="255" g="255" b="255" a="255"/>
		<MinEndColor r="255" g="255" b="255" a="255"/>
		<MaxEndColor r="255" g="255" b="255" a="125"/>
		<Fade in="0.1" out="0.1" type="percentage"/>
		<ExternalImpulseWeight min="0" max="0"/>
	</ParticleEmitter>
	<ParticleEmitter max_particles="4" x="0" y="0" delay="0" space="local" ignore_fog="false">
		<RenderGroup value="350"/>
		<Texture filename="particles/generic_hardedge_drip.png"/>
		<FrameAnimations framesize="0">
			<Animation start="0" end="9" fps="30" type="loop"/>
		</FrameAnimations>
		<EmissionCircle radius="1.2" innerradius="0.5"/>
		<ExternalForce min_x="0" max_x="0" min_y="0.1" max_y="0.1"/>
		<BlendMode type="add"/>
		<BlendModeAlpha type="lightmask"/>
		<PreGeneration time="0" step="0"/>
		<Emission particles="10" type="timed" lifetime="5"/>
		<ParticleOrientation type="rotation"/>
		<Velocity min_x="0" max_x="0" min_y="0" max_y="0"/>
		<VelocityFriction min="0" max="0"/>
		<ParticleLife min="0.5" max="0.7"/>
		<Size min_start="0.125" max_start="0.175" min_end="0.1" max_end="0.1"/>
		<Weight min_start="1" max_start="1" min_end="1" max_end="1"/>
		<Rotation min_start="-180" max_start="180" min_speed="0" max_speed="0"/>
		<RotationForce min="0" max="0"/>
		<RotationFriction min="0" max="0"/>
		<Frame min_start="0" max_start="10"/>
		<MinStartColor r="255" g="255" b="255" a="136"/>
		<MaxStartColor r="255" g="255" b="255" a="255"/>
		<MinEndColor r="255" g="255" b="255" a="255"/>
		<MaxEndColor r="255" g="255" b="255" a="132"/>
		<Fade in="0.1" out="0.6" type="percentage"/>
		<ExternalImpulseWeight min="0" max="0"/>
	</ParticleEmitter>
</ParticleTrigger>
