<?xml version="1.0" encoding="iso-8859-1"?>
<ParticleTrigger xmlns="http://intranet.twotribes.com/schemas/common/particletrigger.xsd" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://intranet.twotribes.com/schemas/common/particletrigger.xsd http://intranet.twotribes.com/schemas/common/particletrigger.xsd">
	<GameEvent name="collectible_collected" max_particles="100"/>
	<ParticleCulling enabled="true"/>
	<ParticleEmitter max_particles="10" x="0" y="0" delay="0" space="local" ignore_fog="false">
		<RenderGroup value="200"/>
		<Texture filename="particles/generic_spark_motion_slim.png"/>
		<FrameAnimations framesize="0">
			<Animation start="0" end="0" fps="30" type="loop"/>
		</FrameAnimations>
		<EmissionCircle radius="0.18" innerradius="0"/>
		<ExternalForce min_x="0" max_x="0" min_y="-0.06" max_y="-0.06"/>
		<BlendMode type="add"/>
		<BlendModeAlpha type="lightmask"/>
		<PreGeneration time="0" step="0"/>
		<Emission particles="10" type="burst" lifetime="0.3"/>
		<ParticleOrientation type="from_origin"/>
		<Velocity min_x="1.5" max_x="4.5" min_y="0" max_y="0"/>
		<VelocityFriction min="0" max="0.15"/>
		<ParticleLife min="0.25" max="0.35"/>
		<Size min_start="0.09" max_start="0.21" min_end="0.015" max_end="0.075"/>
		<Weight min_start="0" max_start="0" min_end="0.75" max_end="1.25"/>
		<Rotation min_start="0" max_start="0" min_speed="0" max_speed="0"/>
		<RotationForce min="0" max="0"/>
		<RotationFriction min="0" max="0"/>
		<Frame min_start="0" max_start="4"/>
		<DirectionFromSpawnLocation/>
		<MinStartColor r="0" g="212" b="255" a="255"/>
		<MaxStartColor r="148" g="0" b="255" a="255"/>
		<MinEndColor r="8" g="255" b="0" a="255"/>
		<MaxEndColor r="255" g="0" b="0" a="255"/>
		<Fade in="0.1" out="0.3" type="percentage"/>
		<ExternalImpulseWeight min="0" max="0"/>
	</ParticleEmitter>
</ParticleTrigger>
