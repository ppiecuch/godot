<?xml version="1.0" encoding="iso-8859-1"?>
<ParticleTrigger xmlns="http://intranet.twotribes.com/schemas/common/particletrigger.xsd" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://intranet.twotribes.com/schemas/common/particletrigger.xsd http://intranet.twotribes.com/schemas/common/particletrigger.xsd">
	<ParticleCulling enabled="true"/>
	<ParticleEmitter max_particles="15" x="0.85" y="-0.8" delay="0" space="world" ignore_fog="false">
		<RenderGroup value="200"/>
		<Texture filename="particles/generic_dust_puff.png"/>
		<FrameAnimations framesize="0">
			<Animation start="0" end="0" fps="30" type="loop"/>
		</FrameAnimations>
		<EmissionRect width="0" height="0"/>
		<ExternalForce min_x="-0.005" max_x="0.005" min_y="0.04" max_y="0.06"/>
		<BlendMode type="blend"/>
		<PreGeneration time="0" step="0"/>
		<Emission particles="10" type="continuous" lifetime="1"/>
		<ParticleOrientation type="rotation"/>
		<Velocity min_x="0" max_x="0" min_y="-1" max_y="-1"/>
		<VelocityFriction min="0" max="0"/>
		<ParticleLife min="0.95" max="1.45"/>
		<Size min_start="0.125" max_start="0.275" min_end="0.75" max_end="1.25"/>
		<Weight min_start="1" max_start="1" min_end="0" max_end="0"/>
		<Rotation min_start="-180" max_start="180" min_speed="-200" max_speed="200"/>
		<RotationForce min="0" max="0"/>
		<RotationFriction min="0" max="0"/>
		<Frame min_start="0" max_start="0"/>
		<MinStartColor r="180" g="180" b="202" a="255"/>
		<MaxStartColor r="214" g="196" b="218" a="255"/>
		<MinEndColor r="40" g="44" b="48" a="255"/>
		<MaxEndColor r="35" g="31" b="36" a="255"/>
		<Fade in="0" out="0.3" type="time"/>
		<ExternalImpulseWeight min="0" max="0"/>
	</ParticleEmitter>
	<ParticleEmitter max_particles="8" x="0.5" y="-0.8" delay="0" space="world" ignore_fog="false">
		<RenderGroup value="200"/>
		<Texture filename="particles/ring_perspective.png"/>
		<FrameAnimations framesize="0">
			<Animation start="0" end="0" fps="30" type="loop"/>
		</FrameAnimations>
		<EmissionRect width="0" height="0"/>
		<ExternalForce min_x="0" max_x="0" min_y="0" max_y="0"/>
		<BlendMode type="add"/>
		<BlendModeAlpha type="lightmask"/>
		<PreGeneration time="0" step="0"/>
		<Emission particles="50" type="continuous" lifetime="1"/>
		<ParticleOrientation type="to_origin"/>
		<Velocity min_x="0" max_x="0" min_y="-6.4" max_y="-6.4"/>
		<VelocityFriction min="0" max="0"/>
		<ParticleLife min="0.15" max="0.15"/>
		<Size min_start="1.04" max_start="1.36" min_end="0" max_end="0.16"/>
		<Weight min_start="1" max_start="1" min_end="1" max_end="1"/>
		<Rotation min_start="0" max_start="0" min_speed="0" max_speed="0"/>
		<RotationForce min="0" max="0"/>
		<RotationFriction min="0" max="0"/>
		<Frame min_start="0" max_start="0"/>
		<MinStartColor r="255" g="0" b="221" a="127"/>
		<MaxStartColor r="255" g="107" b="255" a="127"/>
		<MinEndColor r="255" g="255" b="255" a="134"/>
		<MaxEndColor r="255" g="255" b="255" a="129"/>
		<Fade in="0" out="0.05" type="time"/>
		<ExternalImpulseWeight min="0" max="0"/>
	</ParticleEmitter>
	<ParticleEmitter max_particles="4" x="0.5" y="-0.8" delay="0" space="world" ignore_fog="false">
		<RenderGroup value="200"/>
		<Texture filename="particles/ring_perspective.png"/>
		<FrameAnimations framesize="0">
			<Animation start="0" end="0" fps="30" type="loop"/>
		</FrameAnimations>
		<EmissionRect width="0" height="0"/>
		<ExternalForce min_x="0" max_x="0" min_y="0" max_y="0"/>
		<BlendMode type="add"/>
		<BlendModeAlpha type="lightmask"/>
		<PreGeneration time="0" step="0"/>
		<Emission particles="25" type="continuous" lifetime="1"/>
		<ParticleOrientation type="to_origin"/>
		<Velocity min_x="0" max_x="0" min_y="-2" max_y="-2"/>
		<VelocityFriction min="0" max="0"/>
		<ParticleLife min="0.6" max="0.6"/>
		<Size min_start="1.04" max_start="1.36" min_end="0" max_end="0.16"/>
		<Weight min_start="1" max_start="1" min_end="1" max_end="1"/>
		<Rotation min_start="0" max_start="0" min_speed="0" max_speed="0"/>
		<RotationForce min="0" max="0"/>
		<RotationFriction min="0" max="0"/>
		<Frame min_start="0" max_start="0"/>
		<MinStartColor r="255" g="0" b="221" a="127"/>
		<MaxStartColor r="255" g="107" b="255" a="127"/>
		<MinEndColor r="255" g="255" b="255" a="134"/>
		<MaxEndColor r="255" g="255" b="255" a="129"/>
		<Fade in="0" out="0.05" type="time"/>
		<ExternalImpulseWeight min="0" max="0"/>
	</ParticleEmitter>
</ParticleTrigger>
